<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controls="using:Atomex.Client.Desktop.Styles">
    <Design.PreviewWith>
        <controls:TypeTest />
    </Design.PreviewWith>

    <Style Selector="controls|TypeTest">
        <!-- Set Defaults -->
        <Setter Property="Template">
            <ControlTemplate>
                <ContentPresenter Content="{TemplateBinding Content}"></ContentPresenter>
            </ControlTemplate>
        </Setter>
    </Style>
</Styles>
